
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca953x_wmac'
	option htmode 'HT20'
	option txpower '18'
	option country 'AU'
	option channel '6'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'psk-mixed'
	option key 'Don'\''t you wish you were this good?'
	option ssid 'SmartServices'
	option network 'lan'

config wifi-iface
	option device 'radio0'
	option ifname 'mesh0'
	option network 'mesh'
	option ssid 'mesh'
	option mode 'adhoc'
	option bssid '02:CA:FE:CA:CA:40'
	option mcast_rate '18000'
	option key '1'
	option encryption 'none'

